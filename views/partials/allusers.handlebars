<p>Hi, {{user.name}}</p>
<h2>All Users</h2>
<div id ="contenttable">
    <table class="table table-inverse table-bordered table-condensed table-striped table-hover" id="note-data">
        <thead>
            <tr>
                <th>User</th>
                <th>Points</th>
            </tr>
        </thead>
        <tbody>
            <p>{{a}}</p>
            {{#each userList}}
            <tr class='clickable-row' data-href="/profile/{{_id}}">
                <td>{{name}}</td>
                <td>{{points}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>
<button type="submit" class="btn btn-default" id="addnewnote">Add new note</button>